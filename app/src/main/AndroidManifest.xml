<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rough">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_logo_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/app_logo_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.Rough"
        android:screenOrientation="sensorPortrait"

        >
        <activity android:name=".Leaderboard"></activity>
        <activity android:name=".YourScore" />
        <activity android:name=".frontPage" />
        <activity android:name=".rules" />
        <activity android:name=".MainActivity" />
        <activity android:name=".continue_with_google">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>